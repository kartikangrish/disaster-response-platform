{
  "name": "disaster-response-platform",
  "version": "1.0.0",
  "description": "MERN stack disaster response coordination platform with real-time data aggregation",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "socket.io": "^4.7.4",
    "@supabase/supabase-js": "^2.38.4",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "multer": "^1.4.5-lts.1",
    "express-rate-limit": "^7.1.5",
    "winston": "^3.11.0",
    "uuid": "^9.0.1",
    "node-cron": "^3.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "disaster-response",
    "mern-stack",
    "geospatial",
    "real-time",
    "supabase",
    "gemini-api"
  ],
  "author": "Disaster Response Team",
  "license": "MIT"
} 